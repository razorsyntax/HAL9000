{
	"name": "hal9000js",
	"version": "2.0.2",
	"description": "A JavaScript neural network library with hardware acceleration support",
	"main": "dist/hal9000.cjs.js",
	"module": "dist/hal9000.esm.js",
	"browser": "dist/hal9000.umd.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/hal9000.esm.js",
			"require": "./dist/hal9000.cjs.js",
			"browser": "./dist/hal9000.umd.js",
			"types": "./dist/types/index.d.ts"
		}
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "node build.js",
		"build:watch": "node build.js --watch",
		"test": "node tests/integration/test-cjs.js",
		"demo": "npx serve demo -p 8000",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"neural-network",
		"machine-learning",
		"ai",
		"gpu",
		"tensorflow",
		"mnist",
		"classification"
	],
	"author": "David R. Schwartz <david@biomance.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/razorsyntax/HAL9000.git"
	},
	"homepage": "https://github.com/razorsyntax/HAL9000#readme",
	"bugs": {
		"url": "https://github.com/razorsyntax/HAL9000/issues"
	},
	"dependencies": {},
	"optionalDependencies": {
		"gpu.js": "^2.16.0",
		"@tensorflow/tfjs-node": "^4.10.0",
		"@tensorflow/tfjs-node-gpu": "^4.10.0"
	},
	"devDependencies": {
		"esbuild": "^0.19.0",
		"serve": "^14.2.0"
	},
	"peerDependencies": {},
	"engines": {
		"node": ">=14.0.0"
	}
}

