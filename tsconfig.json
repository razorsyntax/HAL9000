{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"allowJs": true,
		"checkJs": false,
		"noEmit": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"dist",
		"examples",
		"tests",
		"demo",
		"script",
		"build.js"
	]
}

